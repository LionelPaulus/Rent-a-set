<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Place</th>
      <th>Price</th>
      <th>Total</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.user %></td>
        <td><%= booking.place %></td>
        <td><%= booking.price %></td>
        <td><%= booking.total %></td>
        <td><%= booking.start_time %></td>
        <td><%= booking.end_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
