<p id="notice"><%= notice %></p>

<h1>Sets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sets.each do |set| %>
      <tr>
        <td><%= link_to 'Show', set %></td>
        <td><%= link_to 'Edit', edit_set_path(set) %></td>
        <td><%= link_to 'Destroy', set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Set', new_set_path %>
